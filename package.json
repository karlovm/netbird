  "license": "MIT",
  "devDependencies": {
    "electron": "^37.2.3",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "electron-chrome-extensions": "^4.9.0",
    "electron-store": "^8.1.0",
    "path": "^0.12.7"
  }
}
