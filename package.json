{
  "name": "netbird-browser",
  "version": "0.0.1",
  "description": "netbird - a modern electron-based browser",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "pack": "electron-builder --dir"
